<div class="form">
  <h3 class="form-header">管理者ログイン</h3>

  <div class="form-area">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <table>
        <tr>
          <td>メールアドレス</td>
          <td><%= f.email_field :email, autofocus: true, autocomplete: "メールアドレス" %></td>
        </tr>
        <tr>
          <td>パスワード</td>
          <td><%= f.password_field :password, autocomplete: "パスワード" %></td>
        </tr>
      </table>
  </div>
  <div class="form-footer">
    <%= f.submit "ログイン", class:"btn btn-primary" %>
  </div>

<% end %>
</div>