<h3>会員一覧</h3>

<!-- 検索バー 未完成 -->
<div>
  <%= search_form_for @search do |f| %>
    <%= f.search_field :name_cont, placeholder: '会員名で検索' %>
    <%= f.submit ("検索") , class: 'd-btn'%>
  <% end %>
</div>
<div>
	<%= @user.each do |users| %>
	<tr>
		<th>名前</th>
		<th>名前（カナ）</th>
	</tr>
	<tr>
		<th><%= users.name %></th>
		<th><%= users.name_kana %></th>
	</tr>
	<% end %>
</div>
