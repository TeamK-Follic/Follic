<!--  <p>アーティストを選択してください。</p> -->
  <% items.each do |item| %>
  <%= link_to item_path(item) do %>
    <div class="f-list clearfix">
      <div class="f-list-thumbnail">
        <%= attachment_image_tag item, :image, :fill, 40, 40, fallback: "no_image.png"%>

      </div>

      <p class="f-list-text">
        <%= item.type.name %>
        <p>NEW</p>
        <%= item.title %>
      </p>
    </div>
    <% end %>
  <% end %>